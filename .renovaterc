{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "constraints": {
    "go": "1.25"
  },
  "extends": [
    "config:recommended",
    ":semanticCommitTypeAll(build)",
    ":separatePatchReleases"
  ],
  "ignorePresets": [
    ":prHourlyLimit2",
    ":semanticPrefixFixDepsChoreOthers",
    "workarounds:all"
  ],
  "enabledManagers": [
    "gomod",
    "npm"
  ],
  "labels": [
    "dependencies"
  ],
  "postUpdateOptions": [
    "gomodTidy",
    "gomodMassage"
  ]
}
